{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start:test": {
      "inputs": [
        ".env.development"
      ],
      "cache": false,
      "persistent": true
    },
    "start:ot": {
      "env": [".env.ot"],
      "cache": false,
      "persistent": true
    },
    "start:online": {
      "env": [".env.production"],
      "cache": false,
      "persistent": true
    },
    "build:test": {
      "env": [".env.test"],
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:ontest": {
      "env": [".env.ot"],
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:online": {
      "env": [".env.production"],
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
